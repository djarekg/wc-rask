{
  "name": "@wc-rask/web",
  "version": "1.0.0",
  "private": false,
  "author": {
    "name": "Dustin Griffith"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/djarekg/wc-rask"
  },
  "license": "MIT",
  "packageManager": "bun@1.1.41",
  "engines": {
    "bun": ">=1.1.41"
  },
  "type": "module",
  "exports": {
    ".": {
      "@wc-rask/web": "./src/index.ts",
      "import": "./dist/index.js",
      "types": "./src/index.d.ts",
      "default": "./dist/index.js.map",
    },
    "./*": {
      "@wc-rask/web": "./src/*.ts",
      "import": "./dist/*.js",
      "types": "./src/*.d.ts"
    },
    "./module": {
      "types": "./src/types/module.d.ts"
    },
    "./src/module/*": "./src/module/*",
    "./events": {
      "types": "./src/types/events.d.ts"
    },
    "./src/events/*": "./src/events/*",
    "./package.json": "./package.json"
  },
  "scripts": {
    "prebuild": "bunx rimraf dist",
    "build": "jspm build --config jspm.config.mjs",
    "postbuild": "bun run build:declaration",
    "build:declaration": "tsc --project tsconfig.types.json"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4"
  }
}
