  "private": false,
  "author": {
    "name": "Dustin Griffith"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/djarekg/wc-rask"
  },
  "license": "MIT",
  "packageManager": "bun@1.1.41",
  "engines": {
    "bun": ">=1.1.41"
  },
  "type": "module",
  "imports": {
