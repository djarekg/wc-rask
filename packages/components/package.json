{
  "name": "@wc-rask/components",
  "version": "1.0.0",
  "private": false,
  "license": "MIT",
  "packageManager": "bun@1.1.41",
  "author": {
    "name": "Dustin Griffith"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/djarekg/wc-rask"
  },
  "engines": {
    "bun": ">=1.1.41"
  },
  "type": "module",
  "imports": {
    "internal": {
      "#/*": "./src/*"
    }
  },
  "exports": {
    "./*": {
      "@wc-rask/components": "./src/**/*.ts",
      "import": "./dist/**/*.js",
      "types": "./dist/**/*.d.ts"
    },
    "./button": {
      "import": "./dist/button/index.js",
      "types": "./dist/button/button.d.ts"
    },
    "./notification": {
      "import": "./dist/notification/index.js",
      "types": "./dist/notification/notification.d.ts"
    },
    "./text-field": {
      "import": "./dist/text-field/index.js",
      "types": "./src/text-field/text-field.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "prebuild": "bunx rimraf dist",
    "build": "jspm build --config jspm.config.mjs",
    "postbuild": "bun run build:declaration",
    "build:declaration": "tsc --project tsconfig.types.json"
  },
  "dependencies": {
    "@lit-labs/signals": "^0.1.1",
    "@material/web": "^2.2.0",
    "@wc-rask/web": "workspace:*",
    "lit": "^3.2.1",
    "signal-polyfill": "^0.2.1",
    "signal-utils": "^0.21.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
